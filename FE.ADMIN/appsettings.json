{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/myCheatinglog-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ServiceURLs": {
    "ApiKM58": "https://localhost:5001",
    "AuthAPIBase": "https://api-auth.attapp.me"
  }
}
